{
  "expo": {
    "name": "WasteBuster",
    "slug": "wastebuster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/miscellaneous/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/miscellaneous/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#018749"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.williamcagas.wastebuster"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.williamcagas.wastebuster"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "48c9ca60-25af-4388-a2c6-7ef2dd85f9a7"
      }
    },
    "owner": "williamcagas",
    "updates": {
      "url": "https://u.expo.dev/48c9ca60-25af-4388-a2c6-7ef2dd85f9a7"
    },
    "runtimeVersion": "appVersion"
  }
}
